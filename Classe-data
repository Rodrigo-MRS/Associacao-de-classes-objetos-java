public class Data {
    //Atributos
    public int dia;
    public int mes;
    public int ano;
    
    //Construtores
    public Data(int d, int m, int a){
        dia = d;
        mes = m;
        ano = a;
    }
   
    //Método get
    public int getdia(){
        return dia;
    }
    
    public int getmes(){
        return mes;
    }
    
    public int getano(){
        return ano;
    }
    
    //Método set
    
    public void setdia(int novoDia){
        dia = novoDia;
    }
    
    public void setmes(int novoMes){
        mes = novoMes;
    }
    public void setano(int novoAno){
        ano = novoAno;
    }
}
